<!-- all jobs start -->
<section class="section bg-light">
  <div class="container">
    <!-- Header Sentence -->
    <div class="row justify-content-center">
      <div class="col-12">
        <div class="section-title text-center mb-4 pb-2">
          <h4 class="title title-line pb-5">Find Your Mentor</h4>
          <p class="text-muted para-desc mx-auto mb-1">
            Just Search Mentor based on your taste. We'll quickly match you
            with the right Expert.
          </p>
        </div>
      </div>
    </div>

    <!-- Filter Button Group -->
    <div class="row justify-content-center">
      <div class="col-lg-9 text-center mt-4 pt-2">
        <ul
          class="nav nav-pills nav nav-pills bg-white rounded nav-justified flex-column flex-sm-row"
          id="pills-tab"
          role="tablist"
        >
          <li class="nav-item">
            <a
              class="nav-link rounded active"
              id="recent-job-tab"
              data-toggle="pill"
              href="#recent-job"
              role="tab"
              aria-controls="recent-job"
              aria-selected="true"
              >New Expert</a
            >
          </li>
          <li class="nav-item">
            <a
              class="nav-link rounded"
              id="best-expert-tab"
              data-toggle="pill"
              href="#best-expert"
              role="tab"
              aria-controls="best-expert"
              aria-selected="false"
              >Highest Rating</a
            >
          </li>
          <li class="nav-item">
            <a
              class="nav-link rounded"
              id="low-expert-tab"
              data-toggle="pill"
              href="#low-expert"
              role="tab"
              aria-controls="low-expert"
              aria-selected="false"
              >Lowest Price</a
            >
          </li>
        </ul>
      </div>
    </div>

    <!-- List Mentor -->
    <div class="row">
      <div class="col-12">
        <div class="tab-content mt-2" id="pills-tabContent">
          <div
            class="tab-pane fade show active"
            id="recent-job"
            role="tabpanel"
            aria-labelledby="recent-job-tab"
          >
            <!-- Recent-job Loop  -->
            <% newMentor.forEach((mentorData)=>{ %>
            <div class="row">
              <div class="col-lg-12">
                <div
                  class="job-box bg-white overflow-hidden border rounded mt-4 position-relative overflow-hidden"
                  data-aos="zoom-in"
                  data-aos-duration="700"
                >
                  <div class="lable text-center pt-2 pb-2">
                    <ul
                      class="list-unstyled best text-white mb-0 text-uppercase"
                    >
                      <li class="list-inline-item">
                        <i class="mdi mdi-star"></i>
                      </li>
                    </ul>
                  </div>
                  <div class="p-4">
                    <div class="row align-items-center">
                      <div class="col-md-2">
                        <div class="mo-mb-2">
                          <img
                            src="<%= mentorData.profilepicture %>"
                            alt=""
                            class="img-fluid mx-auto d-block"
                            style="width: 45%;"
                          />
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div>
                          <h5 class="f-18">
                            <a href="#" class="text-dark"
                              ><%= mentorData.username %>
                            </a>
                          </h5>
                          <p class="text-muted mb-0">
                            <%= mentorData.job %>
                          </p>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div>
                          <p class="text-muted mb-0">
                            <i class="mdi mdi-map-marker text-primary mr-2"></i
                            ><%= mentorData.city %>
                          </p>
                        </div>
                      </div>
                      <div class="col-md-2">
                        <div>
                          <p class="text-muted mb-0 mo-mb-2">
                            <span class="text-primary"
                              >Rp.<%=
                              currency(mentorData.price,{precision:0}).format()
                              %> /hours
                            </span>
                          </p>
                        </div>
                      </div>
                      <div class="col-md-2">
                        <div>
                          <p class="text-muted mb-0">Full Time</p>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="p-3 bg-light">
                    <div class="row">
                      <div class="col-md-5">
                        <div>
                          <p class="text-muted mb-0 mo-mb-2">
                            <span class="text-dark font-weight-bold"
                              >Experience :</span
                            >
                            <%= mentorData.experience %>
                          </p>
                        </div>
                      </div>
                      <div class="col-md-5">
                        <div>
                          <p class="text-muted mb-0 mo-mb-2">
                            <span class="text-dark font-weight-bold"
                              >Expertise :</span
                            >
                            <%= mentorData.expertise %>
                          </p>
                        </div>
                      </div>
                      <div class="col-md-2">
                        <div>
                          <a
                            href="mdetail/<%=mentorData._id%>"
                            class="text-primary"
                            >See Details
                            <i class="mdi mdi-chevron-double-right"></i
                          ></a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <% } )%>
          </div>
          <!-- End Of Loop -->

          <div
            class="tab-pane fade"
            id="best-expert"
            role="tabpanel"
            aria-labelledby="best-expert-tab"
          >
