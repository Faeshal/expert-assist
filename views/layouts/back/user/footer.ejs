<!-- Data Table Setup -->
<script>
  $(document).ready(function () {
    let url = "/polluserschedule";
    let userId = "<%-session._id%>";
    $("#table_id").DataTable({
      ordering: false,
      stateSave: true,
      pagingType: "simple_numbers",
      language: {
        paginate: {
          first: "«",
          previous: "‹",
          next: "›",
          last: "»",
        },
      },
      responsive: true,
    });

    let subscribe = function () {
      $.ajax({
        method: "GET",
        url: url,
        timeout: 30000,
      })
        .done((data) => {
          console.log(data);
          let id = data.id;
          let message = data.message;
          let messageId = id.toString();
          if (messageId == userId) {
            setTimeout(function () {
              subscribe(url);
              console.log("TRIGERED ALLERT");
              return Swal.fire({
                title: message,
                width: 400,
                icon: "success",
                padding: "3em",
                backdrop: "rgba(0,0,123,0.4)",
                showConfirmButton: false,
                timer: 10000,
              });
            }, 1000);
          }
        })
        .fail((error) => {
          console.log(error);
        });
    };

    subscribe(url);
  });
</script>
