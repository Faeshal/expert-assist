          <ul class="list-group list-group-flush">
            <li class="list-group-item h3">Selamat Mengerjakan</li>
            <li class="list-group-item">
              <p class="font-weight-bold text-danger d-inline-block">Timer :</p>
              <span
                class="badge badge-warning badge-lg display-2 font-weight-bold"
                id="time"
              >
              </span>
            </li>
          </ul>
          <!-- Card body -->
          <div class="card-body">
            <!-- <h3 class="card-title mb-3">Card title</h3> -->
            <div class="card-text mb-4 text-left">
              <div class="container" id="ff-compose"></div>
              <script async defer src="<%=testlink%>"></script>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Footer -->
    <%-include('../../layouts/back/mentor/footer.ejs')%>
  </div>
</div>
