<!-- Trigerred Modals -->
<script>
  $(document).ready(function () {
    let mentorId = "<%-session._id%>";
    $("#modal-notification").modal("show");
    $("#mytable").DataTable({
      ordering: false,
      stateSave: true,
      pagingType: "simple_numbers",
      language: {
        paginate: {
          first: "«",
          previous: "‹",
          next: "›",
          last: "»",
        },
      },
      responsive: true,
    });

    let subscribe = function (url, cb) {
      $.ajax({ method: "GET", url: url, timeout: 30000 })
        .done((data) => {
          cb(data);
          console.log(data);
          let id = data.id;
          let message = data.message;
          let messageId = id.toString();
          if (messageId == mentorId) {
            setTimeout(function () {
              subscribe(url, cb);
              Swal.fire({
                title: message,
                width: 400,
                icon: "success",
                padding: "3em",
                backdrop: "rgba(0,0,123,0.4)",
                showConfirmButton: false,
                timer: 10000,
              });
            }, 1000);
          }
        })
        .fail((err) => {
          console.log(err);
        });
    };

    subscribe("/pollmentorpayment", function (data) {
      console.log("payment Data:", data);
    });

    subscribe("/pollmentorschedule", function (data) {
      console.log("schedule Data:", data);
    });

    subscribe("/pollmentorreview", function (data) {
      console.log("review Data:", data);
    });

    subscribe("/pollmentorwithdraw", function (data) {
      console.log("Withdraw Data:", data);
    });
  });
</script>
